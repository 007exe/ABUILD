pkgname=libunique
pkgver=1.1.6
pkgbuild=1
arch=("auto")

shortdesc=("Library for writing single instance applications for GTK3.")

source=("https://github.com/LeSnake04/aur-lesnake04-sources/raw/main/libunique/libunique-1.1.6-8-x86_64.tar.gz")

tags=("libs dev-libs")

build_deps=("gtk3")

adddep=("gtk3")

build() {
  mv ${srcdir}/usr ${pkgdir}/usr
}
