pkgname=fixesproto
pkgver=5.0
pkgbuild=2
arch=('auto')

shortdesc=('fixesproto')
longdesc=('fixesproto is part of X11. For more information about the X.Org Foundation (the providers of the X.Org implementation of the X Window System), see their website: http://www.x.org')

tags=('x11-protos xserver')

source=("http://xorg.freedesktop.org/archive/individual/proto/fixesproto-${pkgver}.tar.bz2")
build_deps="util-macros"
adddep="xproto xextproto"


BUILD_SYSTEM='autotools'
BUILD_KEYS="--prefix=/usr --libdir=/usr/lib${LIBDIRSUFFIX} --sysconfdir=/etc --localstatedir=/var --datarootdir=/usr --infodir=/usr/info --mandir=/usr/man"
INSTALL_KEYS="DESTDIR=$pkgdir"

