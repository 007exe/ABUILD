<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE default-apps SYSTEM "gnome-da-list.dtd">
<default-apps>
  <web-browsers>
    <web-browser>
      <name>Chromium</name>
      <executable>/usr/bin/chromium</executable>
      <command>/usr/bin/chromium "%s"</command>
      <icon-name>chromium</icon-name>
      <run-in-terminal>false</run-in-terminal>
      <netscape-remote>true</netscape-remote>
      <tab-command>/usr/bin/chromium --new-tab "%s"</tab-command>
      <win-command>/usr/bin/firefox-nightly --new-window "%s"</win-command>
    </web-browser>
  </web-browsers>
</default-apps>
